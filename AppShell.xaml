<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="pordhubertquizy.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:pordhubertquizy"
    Title="pordhubertquizy">

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    ContentTemplate="{DataTemplate local:question}"
    Route="question" />
    <ShellContent
    ContentTemplate="{DataTemplate local:playerturn}"
    Route="playerturn" />
    <ShellContent
    ContentTemplate="{DataTemplate local:leaderboard}"
    Route="leaderboard" />

</Shell>
